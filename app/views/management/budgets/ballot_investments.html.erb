<table>
<% @budgets.each do |budget| %>
  <tr id="<%= dom_id(budget) %>">
    <td><%= budget.name %></td>
    <td><%= budget.translated_phase %></td>
    <td align="right">
      <%= link_to t("management.budgets.manage_ballot"),
                  new_management_budget_ballot_path(budget) %>
    </td>
  </tr>
<% end %>
</table>
